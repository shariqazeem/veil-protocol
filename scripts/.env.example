# Starknet account private key (hex string, with or without 0x prefix)
PRIVATE_KEY=0x...

# Starknet account address (hex string)
ACCOUNT_ADDRESS=0x...

# (Optional) RPC endpoint.
# Sepolia (default): https://starknet-sepolia-rpc.publicnode.com
# Mainnet:           https://rpc.starknet.lava.build
STARKNET_RPC_URL=https://starknet-sepolia-rpc.publicnode.com

# (Optional) Override deployed contract addresses for the keeper
# POOL_ADDRESS=0x...
# USDC_ADDRESS=0x053b40a647cedfca6ca84f542a0fe36736031905a9639a7f19a3c1e66bfd5080
# WBTC_ADDRESS=0x00452bd5c0512a61df7c7be8cfea5e4f893cb40e126bdc40aee6054db955129e

# (Optional) Keeper config
# MIN_PENDING=100
# AVNU_API_BASE=https://sepolia.api.avnu.fi

# ZK Verifier contract address (Garaga UltraKeccakZKHonk verifier)
# Set to 0x0 to skip proof verification (dev mode)
ZK_VERIFIER_ADDRESS=0x00e8f49d3077663a517c203afb857e6d7a95c9d9b620aa2054f1400f62a32f07

# (Optional) Relayer config
# RELAYER_PORT=3001
# RELAYER_FEE_BPS=200

# (Optional) Telegram bot (Veil Strategist)
# Get token from @BotFather on Telegram
# TELEGRAM_BOT_TOKEN=123456:ABC-DEF1234ghIkl-zyx57W2v1u123ew11
# WEB_APP_URL=https://theveilprotocol.vercel.app

# (Optional) Binary paths â€” override for Docker/CI deployments
# GARAGA_BIN=/usr/local/bin/garaga
# NARGO_BIN=/root/.nargo/bin/nargo
# BB_BIN=/root/.bb/bb
# CIRCUITS_DIR=/app/circuits
